<div class="d-flex flex-column flex-md-row align-items-center p-3 px-md-4 mb-3 bg-white border-bottom box-shadow">
  <a class="#" href="/">
    <img src="https://s3.amazonaws.com/himama2/images/horizontal-logo.png" height="30" alt="mdb logo">
  </a>
  <%= link_to "Entries",  entry_details_index_path, class: "btn btn-outline-info d-none d-lg-inline-block mb-3 mb-md-0 ml-md-3" %>
  <ul class="navbar-nav flex-row ml-md-auto d-none d-md-flex">
    <li class="nav-item">
      <% if user_signed_in? %>
        <%= link_to current_user.email, edit_user_registration_path, class: "btn btn-outline-primary" %>
        <%= link_to "Logout", destroy_user_session_path, class: "btn btn-outline-primary", method: :delete %>
      <% else %>
        <%= link_to "Sign Up", new_user_registration_path, class: "btn btn-outline-primary" %>
        <%= link_to "Login", new_user_session_path, class: "btn btn-outline-primary" %>
      <% end %>
    </li>
  </ul>
</div>